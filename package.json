{
  "name": "quran-api",
  "version": "1.0.0",
  "description": "Static Quran API data",
  "main": "./dist/_server/app.js",
  "scripts": {
    "dev:format": "dprint fmt --config dprint.config.json",
    "dev:format:watch": "watchexec --clear --watch . \"pnpm run dev:format\"",
    "dev:lint": "eslint .",
    "dev:watch": "tsx watch src/_server/app.ts",
    "dev:watch:crawler": "tsx watch _crawler/crawler.ts",
    "crawl": "tsx _crawler/crawler.ts"
  },
  "keywords": [
    "quran",
    "quran-api",
    "islamic",
    "muslim-app"
  ],
  "author": {
    "name": "Rizky Aulia Tumangger",
    "url": "https://github.com/rzkytmgr"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "winston": "^3.17.0",
    "yocto-spinner": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.15.3",
    "dprint": "^0.49.1",
    "eslint": "^9.26.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
