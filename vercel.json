{
  "version": 2,
  "buildCommand": "pnpm dev:test && pnpm dev:lint && pnpm build",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "dist/src/_server/app.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "dist/src/_server/app.js"
    }
  ]
}